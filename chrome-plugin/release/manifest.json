{
    "manifest_version": 2, //固定的
    "name": "抢单助手", //插件名称
    "version": "0.0.1", //插件使用的版本
    "description": "陆金所抢单助手", //插件的描述
    "permissions": [ //允许插件访问的url
        "notifications", "tabs", "http://*/*", "https://*/*"
    ],
    "browser_action": { //插件加载后生成图标
        "default_icon": "lu.png", //图标的图片
        "default_popup": "popup.html", //单击图标执行的文件
        "default_title": "陆金所抢单助手" //鼠标移到图标显示的文字
    }
