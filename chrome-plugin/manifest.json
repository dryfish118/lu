{
    "manifest_version": 2,
    "name": "抢单助手",
    "version": "0.0.1",
    "description": "陆金所抢单助手",
    "permissions": [
        "storage", "tabs", "http://*/*", "https://*/*"
    ],
    "options_page": "options.html",
    "background": {
        "scripts": ["jquery-3.2.0.min.js", "background.js"]
    },
    "browser_action": {
        "default_icon": "lu.png",
        "default_title": "陆金所抢单助手"
    },
    "content_scripts": [{
        "matches": ["https://*/*"],
        "js": ["jquery-3.2.0.min.js", "content.js"],
        "run_at": "document_start"
    }]
}